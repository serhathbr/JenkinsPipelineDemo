pipeline {
	agent any

	stages {
		stage ('Hello'){
			steps	{
				echo 'Hello Woeld'
			}
		}
		stage ('Build'){
			steps	{
				echo 'Building'
			}
		}
		stage ('Deploy'){
			steps	{
				echo 'Deploying'
			}
		}
		stage ('Test'){
			steps	{
				echo 'Testing'
			}
		}
		stage ('Relase'){
			steps	{
				echo 'Relase'
			}
		}
	}
}
